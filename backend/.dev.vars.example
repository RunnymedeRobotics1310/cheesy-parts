# Supabase connection
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key

# Auth secret for HMAC token signing (generate a random string)
AUTH_SECRET=your-random-secret-key-here

# Database URL for migrations (from Supabase Dashboard)
DATABASE_URL=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# Resend email service
RESEND_API_KEY=your_api_key
ADMIN_EMAIL=admin@example.com

# Frontend URL for CORS (defaults to http://localhost:5173 in development)
# In production, set this to your actual frontend URL
FRONTEND_URL=https://parts.team1310.ca